{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_bronze_cstmr",
						"type": "DatasetReference"
					},
					"name": "srccustmr"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_silver_customers",
						"type": "DatasetReference"
					},
					"name": "sinkcstmr"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          customer_sk as string,",
				"          customer_id as string,",
				"          first_name as string,",
				"          last_name as string,",
				"          email as string,",
				"          residential_location as string,",
				"          customer_segment as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srccustmr",
				"srccustmr derive(first_name = trim(first_name),",
				"          last_name = trim(last_name),",
				"          residential_location = trim(residential_location),",
				"          customer_segment = trim(customer_segment)) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinkcstmr"
			]
		}
	}
}