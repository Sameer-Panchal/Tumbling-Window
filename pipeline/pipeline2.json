						{
							"type": "Query",
							"text": "MERGE dbo.dim_customer AS tgt\nUSING stg.customers AS src\nON tgt.customer_sk = src.customer_sk\n\nWHEN MATCHED THEN\n    UPDATE SET\n        tgt.customer_id          = src.customer_id,\n        tgt.first_name           = src.first_name,\n        tgt.last_name            = src.last_name,\n        tgt.email                = src.email,\n        tgt.residential_location = src.residential_location,\n        tgt.customer_segment     = src.customer_segment\n\nWHEN NOT MATCHED BY TARGET THEN\n    INSERT (\n        customer_sk,\n        customer_id,\n        first_name,\n        last_name,\n        email,\n        residential_location,\n        customer_segment\n    )\n    VALUES (\n        src.customer_sk,\n        src.customer_id,\n        src.first_name,\n        src.last_name,\n        src.email,\n        src.residential_location,\n        src.customer_segment\n    );\n"
						}
					],
